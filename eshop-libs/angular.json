    "sag-libs": {
      "projectType": "library",
      "root": "projects/sag-libs",
      "sourceRoot": "projects/sag-libs/src",
      "prefix": "sag-lib-lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/sag-libs/tsconfig.lib.json",
            "project": "projects/sag-libs/ng-package.json",
            "scripts": [
              "node_modules/marked/marked.min.js"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sag-libs/src/test.ts",
            "tsConfig": "projects/sag-libs/tsconfig.spec.json",
            "karmaConfig": "projects/sag-libs/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sag-libs/tsconfig.lib.json",
              "projects/sag-libs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
