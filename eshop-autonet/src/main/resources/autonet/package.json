{
  "name": "autonet",
  "version": "1.0.0",
  "scripts": {
    "angular-json": "node angular.js",
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "yarn run angular-json --env=dev && ng serve -c dev --host 0.0.0.0",
    "es5": "yarn run angular-json --env=dev && ng serve -c es5",
    "add:lib": "yarn add ../../../../../eshop-libs/dist/sag-common ../../../../../eshop-libs/dist/sag-thule ../../../../../eshop-libs/dist/sag-article-search ../../../../../eshop-libs/dist/sag-vehicle-list ../../../../../eshop-libs/dist/sag-table ../../../../../eshop-libs/dist/sag-auth ../../../../../eshop-libs/dist/sag-haynespro ../../../../../eshop-libs/dist/sag-currency ../../../../../eshop-libs/dist/sag-gtmotive ../../../../../eshop-libs/dist/sag-custom-pricing ../../../../../eshop-libs/dist/sag-article-detail ../../../../../eshop-libs/dist/sag-article-list ../../../../../eshop-libs/dist/sag-in-context",
    "build:lib": "cd ../../../../../eshop-libs && yarn install && yarn run build",
    "lib": "yarn run build:lib && yarn run add:lib",
    "build:prod": "yarn run angular-json --env=dev && yarn run build --prod --rebase-root-relative-css-urls=true",
    "build--dev": "yarn run angular-json --env=dev && yarn run lib && ng build --prod --base-href=/autonet/ --rebase-root-relative-css-urls=true",
    "build--prod": "yarn run angular-json --env=prod && yarn run lib && ng build --prod --base-href=/autonet/ --rebase-root-relative-css-urls=true"
  },
  "private": true,
